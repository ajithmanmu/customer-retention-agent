# AWS Configuration
NEXT_PUBLIC_AWS_REGION=us-east-1

# Cognito Configuration (from your existing setup)
NEXT_PUBLIC_USER_POOL_ID=YOUR_USER_POOL_ID
NEXT_PUBLIC_USER_POOL_WEB_CLIENT_ID=YOUR_CLIENT_ID
# Get the client secret from AWS Cognito Console > User Pool > App clients > Your Web Client
NEXT_PUBLIC_USER_POOL_WEB_CLIENT_SECRET=your-client-secret-here

# AgentCore Runtime Configuration
AGENTCORE_RUNTIME_ARN=arn:aws:bedrock-agentcore:us-east-1:YOUR_ACCOUNT_ID:runtime/main-7rUSst3r7E
AGENTCORE_RUNTIME_ID=main-7rUSst3r7E

# Optional: Custom domain for Vercel deployment
NEXT_PUBLIC_APP_URL=https://your-app.vercel.app
